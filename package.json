{
  "name": "dietitian-web",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "packageManager": "pnpm@10.4.1",
  "scripts": {
    "dev": "dotenv -c -- next dev --turbopack",
    "start": "dotenv -c -- next start",
    "build": "next build",
    "format": "prettier --check .",
    "format:write": "prettier --write .",
    "format:quiet": "prettier --list-different .",
    "lint": "eslint src --ext .ts,.tsx",
    "lint:fix": "eslint src --ext .ts,.tsx --fix",
    "lint:quiet": "eslint src --ext .ts,.tsx --quiet",
    "typecheck": "tsc --noEmit --incremental",
    "check": "pnpm lint && pnpm typecheck && pnpm format",
    "check:quiet": "pnpm lint:quiet && pnpm typecheck && pnpm format:quiet",
    "fix": "pnpm lint:fix && pnpm format:write",
    "prepare": "husky"
  },
  "dependencies": {
    "@dnd-kit/core": "^6.3.1",
    "@dnd-kit/modifiers": "^9.0.0",
    "@dnd-kit/sortable": "^10.0.0",
    "@dnd-kit/utilities": "^3.2.2",
    "@formkit/auto-animate": "^0.8.2",
    "@hookform/resolvers": "^3.10.0",
    "axios": "^1.7.9",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "cmdk": "^1.1.1",
    "lucide-react": "^0.468.0",
    "next": "15.0.5",
    "next-themes": "^0.4.4",
    "radix-ui": "^1.4.3",
    "react": "^18.3.1",
    "react-aria-components": "^1.12.2",
    "react-dom": "^18.3.1",
    "react-hook-form": "^7.56.3",
    "sonner": "^1.7.4",
    "spinners-react": "^1.0.10",
    "tailwind-merge": "^2.6.0",
    "tailwindcss-animate": "^1.0.7",
    "vitest": "^2.1.9",
    "zod": "^3.24.2",
    "zustand": "^5.0.3",
    "gsap": "^3.13.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.8.1",
    "@commitlint/config-conventional": "^19.8.1",
    "@types/canvas-confetti": "^1.9.0",
    "@types/js-cookie": "^3.0.6",
    "@types/jsonwebtoken": "^9.0.8",
    "@types/node": "^20.17.19",
    "@types/react": "^19.0.10",
    "@types/react-dom": "^19.0.4",
    "@typescript-eslint/eslint-plugin": "^8.40.0",
    "@typescript-eslint/parser": "^8.40.0",
    "@gsap/react": "^2.1.2",
    "dotenv-cli": "^10.0.0",
    "eslint": "^8.57.1",
    "eslint-config-next": "15.0.5",
    "eslint-config-prettier": "^10.1.2",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-react-hooks": "^5.2.0",
    "husky": "^9.1.7",
    "lint-staged": "^16.1.5",
    "postcss": "^8.5.3",
    "prettier": "^3.5.1",
    "prettier-plugin-tailwindcss": "^0.6.11",
    "shadcn": "^3.4.0",
    "tailwindcss": "^3.4.17",
    "tsx": "^4.20.6",
    "typescript": "^5.7.3"
  }
